# AgentVault Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=production
PORT=4000
API_URL=http://localhost:4000
CLIENT_URL=http://localhost:3000

# Database Configuration
DATABASE_URL="file:./dev.db"
DB_MAX_CONNECTIONS=10
DATABASE_PROVIDER=sqlite

# Redis Configuration (for caching and sessions)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_MAX_RETRIES=3
REDIS_PASSWORD=
REDIS_TLS=false

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d
SESSION_SECRET=your-session-secret-change-this-in-production

# Coinbase CDP Configuration (REQUIRED)
CDP_API_KEY=your-cdp-api-key
CDP_PRIVATE_KEY=your-cdp-private-key
CDP_BASE_URL=https://api.coinbase.com
CDP_NETWORK=base-sepolia
CDP_WALLET_ID=

# Amazon Bedrock Configuration (REQUIRED)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_GUARDRAIL_ID=
BEDROCK_GUARDRAIL_VERSION=

# x402pay Configuration (REQUIRED)
X402_PAY_API_KEY=your-x402pay-api-key
X402_PAY_SECRET_KEY=your-x402pay-secret-key
X402_PAY_BASE_URL=https://api.x402.org
X402_PAY_WEBHOOK_SECRET=your-webhook-secret
X402_PAY_PLATFORM_WALLET=your-platform-wallet-address
X402_PAY_ENABLED=true

# Pinata IPFS Configuration (REQUIRED)
PINATA_JWT=your-pinata-jwt
PINATA_API_KEY=your-pinata-api-key
PINATA_SECRET_API_KEY=your-pinata-secret-api-key
PINATA_GATEWAY_URL=https://gateway.pinata.cloud

# Market Data APIs
COINGECKO_API_KEY=your-coingecko-api-key
COINMARKETCAP_API_KEY=your-coinmarketcap-api-key

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
CORS_ENABLED=true

# Feature Flags
ENABLE_DEMO_MODE=false
ENABLE_PAPER_TRADING=true
ENABLE_REAL_TRADING=false
ENABLE_WEBSOCKET=true

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Agent Configuration
AGENT_EXECUTION_INTERVAL=60000
AGENT_MAX_TRADE_SIZE=1000
AGENT_DEFAULT_SLIPPAGE=0.02
AGENT_MAX_DAILY_TRADES=50

# Email Configuration (optional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
EMAIL_FROM=noreply@agentvault.ai

# Monitoring (optional)
SENTRY_DSN=
DATADOG_API_KEY=

# Deployment Configuration
AKASH_DEPLOY_KEY=
AKASH_PROVIDER_URL=

# Client-side Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000

# Akash Network Deployment
AKASH_KEY_NAME=
AKASH_KEYRING_BACKEND=
AKASH_NODE=
AKASH_CHAIN_ID= 