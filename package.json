{
  "name": "agentvault",
  "version": "1.0.0",
  "description": "Autonomous Crypto Investment Agent Platform",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch src/server --ext ts,json --exec \"npx tsx src/server/index.ts\"",
    "dev:client": "next dev -p 3000",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "next build",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "node dist/server/index.js",
    "start:client": "next start",
    "test": "jest --passWithNoTests",
    "test:e2e": "cypress run",
    "lint": "eslint . src --ext .ts,.tsx,.js",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "check:all": "npm run check && npm run test",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "deploy": "bash scripts/deploy.sh --deploy",
    "deploy:akash": "bash scripts/deploy.sh --akash",
    "docker:build": "docker build -t agentvault:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "health-check": "curl -f http://localhost:4000/health || exit 1",
    "production": "NODE_ENV=production npm start",
    "audit:prod": "npm audit --omit=dev --audit-level=critical",
    "audit:fix": "npm audit fix --force",
    "audit:full": "npm audit && npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "security:update": "npm update && npm audit fix",
    "security:fix": "node scripts/fix-security.js",
    "postinstall": "patch-package",
    "loadtest": "k6 run scripts/loadtest.k6.js"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@aws-sdk/client-bedrock-runtime": "^3.600.0",
    "@coinbase/agentkit": "^0.1.2",
    "@coinbase/agentkit-langchain": "^0.3.0",
    "@coinbase/cdp-sdk": "^1.8.0",
    "@coinbase/wallet-sdk": "^4.0.3",
    "@coinbase/x402": "^0.4.1",
    "@pinata/sdk": "^2.1.0",
    "@prisma/client": "^6.10.1",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.22.0",
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "cookies-next": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "form-data": "^4.0.0",
    "graphql": "^16.8.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-validation-complexity": "^0.4.2",
    "helmet": "^7.1.0",
    "ipfs-http-client": "^60.0.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.4",
    "node-cache": "^5.1.2",
    "patch-package": "^8.0.0",
    "pg": "^8.12.0",
    "prom-client": "^15.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.58.1",
    "recharts": "^3.0.1",
    "uuid": "^10.0.0",
    "viem": "^2.31.6",
    "wagmi": "^2.15.6",
    "winston": "^3.13.0",
    "ws": "^8.17.1",
    "x402-express": "^0.4.1",
    "x402-fetch": "^0.4.1"
  },
  "devDependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
