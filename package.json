{
  "name": "agentvault",
  "version": "1.0.0",
  "description": "Autonomous Crypto Investment Agent Platform",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server/index.ts",
    "dev:client": "next dev -p 3000",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "next build",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "node dist/server/index.js",
    "start:client": "next start",
    "test": "jest",
    "test:e2e": "cypress run",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "deploy": "bash scripts/deploy.sh --deploy",
    "deploy:akash": "bash scripts/deploy.sh --akash",
    "docker:build": "docker build -t agentvault:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "health-check": "curl -f http://localhost:4000/api/health || exit 1",
    "production": "NODE_ENV=production npm start"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@aws-sdk/client-bedrock-runtime": "^3.600.0",
    "@coinbase/cdp-sdk": "^1.16.0",
    "@coinbase/wallet-sdk": "^4.0.3",
    "@prisma/client": "^5.15.0",
    "apollo-server-express": "^3.12.1",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "express": "^4.19.2",
    "graphql": "^16.8.2",
    "helmet": "^7.1.0",
    "ipfs-http-client": "^60.0.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.4",
    "pg": "^8.12.0",
    "prisma": "^5.15.0",
    "rate-limiter-flexible": "^5.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.14",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@heroicons/react": "^2.1.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/stack-utils": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "keywords": [
    "crypto",
    "ai",
    "autonomous",
    "investment",
    "defi",
    "blockchain",
    "agent"
  ],
  "author": "AgentVault Team",
  "license": "MIT"
}
